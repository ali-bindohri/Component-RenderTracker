{
  "manifest_version": 3,
  "name": "React Render Counter",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["http://localhost/*", "http://127.0.0.1/*"],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "developer_mode": true,
  "web_accessible_resources": [
    {
      "resources": ["scripts/inject.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
